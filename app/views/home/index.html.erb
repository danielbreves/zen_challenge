<h1>Home</h1>
<table border="1">
<% @songs.each do |song| %>
	<tr>
		<td><%= link_to song.title, {:controller => 'songs',:action => 'show', :id => song.id} %></td>
		<td><%= song.artist.name %></td>
		<td><%= song.votes %></td>
		<td class="up_vote">
			<%= link_to "Up vote",
				  {:controller => 'songs', :action => 'up_vote', :id => song.id}
			%>
		</td>
	</tr>
<% end %>
</table>

<br />

<%= link_to "Admin", {:controller => 'artists'} %>